{
  "name": "vite_elm_tailwind",
  "author": "Andrea Callea",
  "description": "Elm, Vite, Tailwind CSS + daisyUI, template :)",
  "version": "2.0.0",
  "private": true,
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "autoprefixer": "^10.4.13",
    "elm-review": "^2.8.2",
    "elm-test": "^0.19.1-revision10",
    "elm-tooling": "^1.10.0",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "standard": "^17.0.0",
    "standard-markdown": "^7.1.0",
    "stylelint": "^14.15.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "tailwindcss": "^3.2.4",
    "vite": "^3.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-elm": "^2.7.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-webfont-dl": "^3.4.1"
  },
  "scripts": {
    "postinstall": "elm-tooling install",
    "dev": "vite",
    "prebuild": "elm-tooling install",
    "build": "vite build",
    "preview": "npm run build; vite preview",
    "standard": "standard --fix src/**/*.js",
    "markdown": "standard-markdown --fix src/**/*.md",
    "stylelint": "stylelint --fix 'src/**/*.(css|scss|sass)'",
    "review": "elm-review --fix-all",
    "test": "elm-test-rs"
  },
  "standard": {
    "ignore": [
      "public",
      "dist",
      "elm.js",
      "tmp.js"
    ]
  },
  "dependencies": {
    "daisyui": "^2.41.0"
  }
}
