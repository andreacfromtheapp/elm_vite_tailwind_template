    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "elm-review": "^2.11.1",
    "elm-test": "^0.19.1-revision12",
    "elm-tooling": "^1.15.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "standard": "^17.1.0",
    "standard-markdown": "^7.1.0",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.10",
    "vite-plugin-compression": "^0.5.1",
